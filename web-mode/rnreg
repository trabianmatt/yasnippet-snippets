import {
  default as React,
  AppRegistry,
  StyleSheet,
  View,
} from 'react-native';

const $1 = React.createClass({

  render() {
    return(
      ${2:<View>$3</View>}
    );
  }
  
});

const styles = StyleSheet.create({

});

AppRegistry.registerComponent('$1', () => $1);